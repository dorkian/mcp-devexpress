{
  "properties": [
    {
      "name": "Properties",
      "subMembers": [
        {
          "name": "Configuration",
          "type": null,
          "default": null,
          "description": "This section describes properties that configure Sortable UI component content, behavior, and appearance."
        },
        {
          "name": "allowDropInsideItem",
          "type": "Boolean",
          "default": "false",
          "description": "Allows a user to drop an item inside another item."
        },
        {
          "name": "allowReordering",
          "type": "Boolean",
          "default": "true",
          "description": "Allows a user to reorder sortable items."
        },
        {
          "name": "dragComponent",
          "type": null,
          "default": null,
          "description": "An alias for the [dragTemplate] property specified in React. Accepts a custom component. Refer to [Using a Custom Component] for more information."
        },
        {
          "name": "dragRender",
          "type": null,
          "default": null,
          "description": "An alias for the [dragTemplate] property specified in React. Accepts a rendering function. Refer to [Using a Rendering Function] for more information."
        },
        {
          "name": "dragTemplate",
          "type": "template | undefined",
          "default": "undefined",
          "description": "Specifies custom markup to be shown instead of the item being dragged."
        },
        {
          "name": "dropFeedbackMode",
          "type": "Enums.DragHighlight",
          "default": "'push'",
          "description": "Specifies how to highlight the item's drop position."
        },
        {
          "name": "filter",
          "type": "String",
          "default": "'> *'",
          "description": "Specifies a CSS selector for the items that can be dragged."
        },
        {
          "name": "group",
          "type": null,
          "default": null,
          "description": null
        },
        {
          "name": "itemOrientation",
          "type": "Enums.Orientation",
          "default": "'vertical'",
          "description": "Notifies the UI component of the items' orientation."
        },
        {
          "name": "moveItemOnDrop",
          "type": "Boolean",
          "default": "false",
          "description": "Moves an element in the HTML markup when it is dropped."
        }
      ]
    },
    {
      "name": "dxSortable",
      "type": null,
      "default": null,
      "description": "Sortable is a user interface utility that allows a UI component's items to be reordered via drag and drop gestures."
    }
  ],
  "Methods": [
    {
      "name": "update()",
      "type": null,
      "default": null,
      "description": "Updates Sortable's dimensions. Call this method after items are added or their dimensions are changed during dragging."
    }
  ],
